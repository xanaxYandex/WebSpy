{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "gulp default & electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~13.6.6",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.0.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chart.js": "^2.9.3",
    "electron-packager": "^14.0.6",
    "favicon-getter": "^1.1.3",
    "jsonfile": "^5.0.0",
    "uuid": "^3.3.3"
  }
}
